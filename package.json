{
  "name": "red_tetris",
  "version": "0.0.1",
  "author": "arive-de_&_cbarbier",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "client-dist": "NODE_ENV=production webpack --progress --colors",
    "client-dev": "webpack-dev-server --colors --hot --inline --host 0.0.0.0 --port 8080",
    "srv-dev": "DEBUG=∆:* babel-watch -w src src/server/main.js",
    "srv-dist": "DEBUG=∆:* babel src --out-dir dist",
    "test": "mocha 'src/**/*spec.js' --exit",
    "coverage": "NODE_ENV=test nyc npm run test"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "http-status-codes": "^1.3.2",
    "mocha": "^6.2.2",
    "mongoose": "^5.7.3",
    "node-sass": "^4.12.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.0.1",
    "reactstrap": "^8.0.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^8.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-watch": "^7.0.0",
    "css-loader": "^3.2.0",
    "debug": "^4.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^2.13.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^7.0.0",
    "mockgoose": "^8.0.3",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^3.8.2",
    "webpack-hot-middleware": "^2.10.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "**/*.spec.js"
    ],
    "watch-files": [
      "src/**/*.spec.js"
    ],
    "branches": 50,
    "lines": 70,
    "functions": 70,
    "statements": 70
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
